{
  "name": "angulartics2",
  "description": "Vendor-agnostic web analytics for Angular2 applications",
  "homepage": "https://angulartics.github.io/angulartics2/",
  "author": "Jo√£o Ribeiro <jonnybgod@gmail.com> (http://github.com/JonnyBGod)",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf dist",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "test": "ng test --watch=false --browsers=ChromeCI",
    "test:ci": "ng test --code-coverage --watch=false --no-progress --browsers=ChromeCI",
    "test:watch": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "presemantic-release": "cp -R .git dist/packages-dist",
    "semantic-release": "cd dist/packages-dist && semantic-release",
    "ghpages": "ng build --prod --no-progress --baseHref='/angulartics2/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.6",
    "@angular/cdk": "9.2.3",
    "@angular/common": "9.1.6",
    "@angular/compiler": "9.1.6",
    "@angular/core": "9.1.6",
    "@angular/forms": "9.1.6",
    "@angular/localize": "9.1.6",
    "@angular/material": "9.2.3",
    "@angular/platform-browser": "9.1.6",
    "@angular/platform-browser-dynamic": "9.1.6",
    "@angular/router": "9.1.6",
    "@ng-bootstrap/ng-bootstrap": "6.1.0",
    "@uirouter/angular": "6.0.1",
    "@uirouter/core": "6.0.5",
    "bootstrap": "4.4.1",
    "core-js": "3.6.5",
    "rxjs": "6.5.5",
    "tslib": "1.11.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.5",
    "@angular/cli": "9.1.5",
    "@angular/compiler-cli": "9.1.6",
    "@angular/language-service": "9.1.6",
    "@types/applicationinsights-js": "1.0.9",
    "@types/facebook-pixel": "0.0.20",
    "@types/fs-extra": "8.1.0",
    "@types/google.analytics": "0.0.40",
    "@types/jasmine": "3.5.10",
    "@types/node": "13.13.5",
    "@types/segment-analytics": "0.0.32",
    "codelyzer": "5.2.2",
    "fs-extra": "9.0.0",
    "jasmine-core": "3.5.0",
    "karma": "5.0.5",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.2",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "9.1.3",
    "rimraf": "3.0.2",
    "semantic-release": "17.0.7",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
