{
  "name": "angulartics2",
  "description": "Vendor-agnostic web analytics for Angular2 applications",
  "homepage": "https://angulartics.github.io/angulartics2/",
  "author": "Jo√£o Ribeiro <jonnybgod@gmail.com> (http://github.com/JonnyBGod)",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf dist",
    "build": "ts-node -O '{\"module\": \"commonjs\"}' build.ts",
    "test": "ng test --watch=false --browsers=ChromeCI",
    "test:ci": "ng test --code-coverage --watch=false --no-progress --browsers=ChromeCI",
    "test:watch": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "presemantic-release": "cp -R .git dist/packages-dist",
    "semantic-release": "cd dist/packages-dist && semantic-release",
    "ghpages": "ng build --prod --no-progress --baseHref='/angulartics2/'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.0-beta.11",
    "@angular/cdk": "8.0.0-beta.0",
    "@angular/common": "8.0.0-beta.11",
    "@angular/compiler": "8.0.0-beta.11",
    "@angular/core": "8.0.0-beta.11",
    "@angular/forms": "8.0.0-beta.11",
    "@angular/material": "8.0.0-beta.0",
    "@angular/platform-browser": "8.0.0-beta.11",
    "@angular/platform-browser-dynamic": "8.0.0-beta.11",
    "@angular/router": "8.0.0-beta.11",
    "@ng-bootstrap/ng-bootstrap": "4.1.1",
    "@uirouter/angular": "3.0.0",
    "bootstrap": "4.3.1",
    "core-js": "2.5.4",
    "rxjs": "6.4.0",
    "zone.js": "0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0-beta.7",
    "@angular/cli": "8.0.0-beta.11",
    "@angular/compiler-cli": "8.0.0-beta.11",
    "@angular/language-service": "8.0.0-beta.11",
    "@types/applicationinsights-js": "1.0.9",
    "@types/facebook-pixel": "0.0.19",
    "@types/fs-extra": "5.0.5",
    "@types/google.analytics": "0.0.39",
    "@types/jasmine": "3.3.12",
    "@types/node": "~11.13.2",
    "@types/segment-analytics": "0.0.30",
    "codelyzer": "5.0.0",
    "fs-extra": "7.0.1",
    "jasmine-core": "3.4.0",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "5.0.1",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-node": "8.0.3",
    "tsickle": "0.34.3",
    "tslint": "5.15.0",
    "typescript": "3.3.4000"
  },
  "release": {
    "branch": "master"
  },
